<h2>Others who added <%= @song.artist %> - <%= @song.title %></h2>

<table class="table table-striped">
	<tr>
		<th>Name</th>
		<th>Times Added</th>
	</tr>
	<% @listeners.each do |listener| %>
		<tr>
			<td><a href="/users/<%= listener.user.id %>"><%= listener.user.first_name %> <%= listener.user.last_name %></a></td>
			<td><%= listener.adds %></td>
		</tr>
	<% end %>
</table>